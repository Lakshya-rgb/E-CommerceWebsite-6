package edu.ben.model;

import java.io.Serializable;

public class Transaction implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 5447609802695253120L;
	
	private double total;
	private String nameOnCreditCard;
	private String ccLast4Digits;

	public Transaction(double total, String nameOnCreditCard, String ccLast4Digits) {

		this.total = total;
		this.nameOnCreditCard = nameOnCreditCard;
		this.ccLast4Digits = ccLast4Digits;

	}

	public double getTotal() {
		return total;
	}

	public String getNameOnCreditCard() {
		return nameOnCreditCard;
	}

	public String getCcLast4Digits() {
		return ccLast4Digits;
	}

	public void setTotal(double total) {
		this.total = total;
	}

	public void setNameOnCreditCard(String nameOnCreditCard) {
		this.nameOnCreditCard = nameOnCreditCard;
	}

	public void setCcLast4Digits(String ccLast4Digits) {
		this.ccLast4Digits = ccLast4Digits;
	}
	
	

}
